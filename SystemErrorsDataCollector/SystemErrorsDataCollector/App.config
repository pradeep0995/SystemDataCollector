<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    <section name="Mappings" type="SystemErrorsDataCollector.Helper.MappingSection, SystemErrorsDataCollector" />
  </configSections>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  
   <appSettings>
      <add key="MonthsWindow" value="-2" />
   </appSettings>
  
    <Mappings>
      <ExclusionEvents>
        <add Source ="Netwtw06" EventID="5002"/>
        <add Source ="NETLOGON" EventID="5719"/>
        <add Source ="Microsoft-Windows-DNS-Client" EventID ="1023"/>
      </ExclusionEvents>
  </Mappings>

</configuration>